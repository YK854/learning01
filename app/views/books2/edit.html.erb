<h4>投稿編集</h4>
<table>
  <%= form_with model:@book2, url:books2_path, local:true do |f| %>
    <tr>
      <th><%= f.label :image %></th>
      <td><%= f.attachment_field :image %></td>
    </tr>
    <tr>
      <th><%= f.label :title %></th>
      <td><%= f.text_field :title %></td>
    </tr>
    <tr>
      <th><%= f.label :body %></th>
      <td><%= f.text_field :body %></td>
    </tr>
    <tr>
      <td><%= f.submit '送信' %></td>
    </tr>
  <% end %>
</table>